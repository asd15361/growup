DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_TEXT_MODEL=deepseek-chat
DEEPSEEK_VISION_MODEL=deepseek-chat
DEEPSEEK_BASE_URL=https://api.deepseek.com
MODEL_MAX_TOKENS=2200
AUTH_CACHE_TTL_MS=300000
CHAT_DEBUG_TIMINGS=0
PORT=8787
OPS_METRICS_KEY=
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_LOGIN_MAX=8
RATE_LIMIT_REGISTER_MAX=6
RATE_LIMIT_CHAT_IP_MAX=40
RATE_LIMIT_CHAT_USER_MAX=30
SLOW_REQUEST_MS=1500

# New PocketBase instance (recommended explicit vars to avoid wrong target)
POCKETBASE_URL_NEW=https://your-new-pocketbase-domain
POCKETBASE_USERS_COLLECTION=users
POCKETBASE_CHAT_COLLECTION=growup_chat_messages
POCKETBASE_MEMORIES_COLLECTION=growup_memories
POCKETBASE_USER_APPS_COLLECTION=user_apps
POCKETBASE_APP_ID=mobile
POCKETBASE_APP_ID_WHITELIST=mobile,web,admin
POCKETBASE_DEFAULT_USER_ROLE=member
POCKETBASE_DEFAULT_USER_STATUS=active
POCKETBASE_SUPERUSER_EMAIL=
POCKETBASE_SUPERUSER_PASSWORD=

# Vector retrieval (Qdrant)
VECTOR_ENABLED=true
QDRANT_URL=
QDRANT_API_KEY=
QDRANT_COLLECTION=growup_memories
VECTOR_DIM=384
VECTOR_TOP_K=6
VECTOR_TIMEOUT_MS=1500
VECTOR_BACKOFF_MS=60000
VECTOR_MIN_QUERY_CHARS=4
VECTOR_TEXT_MAX_CHARS=1200

# For Expo client (real device build should point to Sealos API domain)
# Example: https://zhiji-api-xxxx.cloud.sealos.io
EXPO_PUBLIC_API_BASE_URL=http://localhost:8787

